{
    "name": "junipertheme/junipertheme",
    "type": "library",
    "authors": [
        {
            "name": "bartnovak",
            "email": "b.nowak@osomstudio.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Juniper\\": "inc"
        }
    },
    "require": {
        "php": ">=8.1",
        "timber/timber": "^2.0",
        "bartnovak/timber-acf-wp-blocks": "^0.1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "composer/installers": true
        },
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        }
    }
}
